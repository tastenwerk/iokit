<% if( flash.notice && flash.notice.length > 0 ){ %>

  var doc = <%- JSON.stringify( doc ) %>;
  iokit.modal('close');
  var treeView = $('#iokit-folders').data('kendoTreeView');
  if( doc.parentIds && doc.parentIds.length > 0 )
    doc.parentIds.forEach( function( parentId ){
      var parentItem = treeView.dataSource.get( doc.parentIds[0] );
      console.log( parentItem );
      treeView.append( doc, treeView.findByUid(parentItem.uid) );
    });
  else
    treeView.append( doc );

<% } %>
  
iokit.notify( <%- JSON.stringify( flash ) %> );